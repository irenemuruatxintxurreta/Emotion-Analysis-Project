      "source": [
        "## Map HF-Emotion Labels → Go-Emotions Indices"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "8H2JyFoVc8aS",
        "outputId": "0708bc2d-049d-4356-9c61-285959fcfc76"
